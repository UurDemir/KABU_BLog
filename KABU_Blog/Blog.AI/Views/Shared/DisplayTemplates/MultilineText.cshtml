@model string

<div class="control-group">
    @Html.LabelFor(x => x, new { @class = "control-label" })
    <div class="controls">
        <textarea class="input-xlarge" disabled="disabled" rows="5"  id="@Html.IdFor(x=> x)" name="@Html.NameFor(x=> x)">@Html.ValueFor(x => x)</textarea>
    </div>
</div>
